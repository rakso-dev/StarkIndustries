<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>IngenieroServlet</servlet-name>
    <servlet-class>web.IngenieroServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>FabricanteServlet</servlet-name>
    <servlet-class>web.FabricanteServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>IngenieroServlet</servlet-name>
    <url-pattern>/Ingeniero</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>FabricanteServlet</servlet-name>
    <url-pattern>/Fabricante</url-pattern>
  </servlet-mapping>

</web-app>
